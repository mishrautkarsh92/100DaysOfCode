//https://leetcode.com/problems/binary-search/
class Solution {
    public int search(int[] nums, int target) {
      int index = -1;
	    int start = 0, end = nums.length-1;
	    while(start<=end) {
	    	int mid = (start+end)/2;
	    	if(nums[mid]==target)
	    		return mid;
	    	else if(nums[mid]>target)
	    		end = mid-1;
	    	else if(nums[mid]<target)
	    		start = mid+1;
	    }
	    return index;
    }
}
